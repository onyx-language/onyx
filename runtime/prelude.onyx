enum Result<R, E> {
    Ok(r: R),
    Err(e: E),
}

class String {
    private var data: [u8];
    private var length: usize;
    public function create(data: [u8]) -> String {
        this.data = data;
        this.length = length.size();
    }
    public function length(this) -> usize => this.length;
}