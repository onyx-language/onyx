enum Result<R, E> {
    Ok(value: R),
    Err(value: E),
}

class String {
    private var data: [u8];
    private var length: usize;
    public function create(data: [u8]) -> raw String {
        this.data = data;
        this.length = data.size();
    }
    public function length(this) -> usize => this.length;
}


function foo() -> Result<i32, [char]> {
    return Result::Ok<i32>(42);
}
